@using Microsoft.AspNet.Identity
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="images/favicon.ico" type="image/ico" />

    <title>Đá Phong Thủy</title>

    <!-- Bootstrap -->
    <link href="/Areas/Admin/template/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="/Areas/Admin/template/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="/Areas/Admin/template/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- iCheck -->
    <link href="/Areas/Admin/template/vendors/iCheck/skins/flat/green.css" rel="stylesheet">

    <!-- bootstrap-progressbar -->
    <link href="/Areas/Admin/template/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="/Areas/Admin/template/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
    <!-- bootstrap-daterangepicker -->
    <link href="/Areas/Admin/template/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">

    <!-- Datatables -->
    <link href="/Areas/Admin/template/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
    <link href="/Areas/Admin/template/vendors/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet">
    <link href="/Areas/Admin/template/vendors/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css" rel="stylesheet">
    <link href="/Areas/Admin/template/vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
    <link href="/Areas/Admin/template/vendors/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">

    <!-- Custom Theme Style -->
    <link href="/Areas/Admin/template/build/css/custom.min.css" rel="stylesheet">

    <script src="~/ckeditor_4.10.0_full/ckeditor/ckeditor.js"></script>

    <script src="~/ckfinder/ckfinder.js"></script>
</head>

<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="@Url.Action("Index","HomeAdmin",new {area="Admin" })" class="site_title"><i class="fa fa-paw"></i> <span>Trang Admin</span></a>
                    </div>

                    <div class="clearfix"></div>

                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_pic">
                            <img src="https://vignette.wikia.nocookie.net/dragonvale/images/b/bb/Admin.gif/revision/latest?cb=20120130151927" alt="..." class="img-circle profile_img">
                        </div>
                        <div class="profile_info">
                            <span>Xin Chào </span>
                            <h2>@User.Identity.GetUserName()</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->

                    <br />

                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <h3>General</h3>
                            <ul class="nav side-menu">
                                <li>
                                    <a><i class="fa fa-home"></i> Quản Lý Thành Viên <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="@Url.Action("Index","ManageUser",new {area="Admin" })">Danh Sách Thành Viên</a></li>
                                        <li><a href="@Url.Action("Index","ManageRole",new {area="Admin" })">Danh Sách Quyền</a></li>
                                    </ul>
                                </li>

                                <li>
                                    <a href="@Url.Action("Categories","ManageCategory",new {area="Admin" })"><i class="fa fa-desktop"></i>Danh Mục Sản Phẩm<span></span></a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Products","ManageProduct",new {area="Admin" })"><i class="fa fa-edit"></i> Quản Lý Sản Phẩm</a>
                                    @*@{Html.RenderAction("_AdminMenuCategoryPartial", "HomeAdmin", new { area = "Admin" });}*@
                                </li>
                                <li>
                                    <a href="@Url.Action("Index","ManageNewCategory",new {area="Admin" })"><i class="fa fa-table"></i> Danh Mục Tin Tức <span></span></a>
                                </li>
                                <li>
                                    <a><i class="fa fa-bar-chart-o"></i>Quản Lý Tin Tức <span class="fa fa-chevron-down"></span></a>
                                    @{Html.RenderAction("_AdminMenuNewCategoryPartial", "HomeAdmin", new { area = "Admin" });}
                                </li>
                                <li>
                                    <a><i class="fa fa-clone"></i>Đơn Hàng & Khách Hàng<span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="@Url.Action("Index","ManageOrder",new { area="Admin"})">Danh Sách Đơn Hàng</a></li>
                                        <li><a href="@Url.Action("ListCustomers","ManageOrder",new { area="Admin"})">Danh Sách Khách Hàng</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <div class="menu_section">
                            <h3>Live On</h3>
                            <ul class="nav side-menu">
                                <li>
                                    <a href="@Url.Action("Index","ManageMenuType",new {area="Admin" })"><i class="fa fa-bug"></i> Danh Mục Menu <span class="fa fa-chevron-down"></span></a>
                                   
                                </li>
                                <li>
                                    <a><i class="fa fa-windows"></i> Quản Lý Menu <span class="fa fa-chevron-down"></span></a>
                                    @{Html.RenderAction("_ListMenuTypePartial", "ManageMenu", new { area = "Admin" });}
                                </li>
                                @*<li>
                                    <a><i class="fa fa-sitemap"></i> Multilevel Menu <span class="fa fa-chevron-down"></span></a>
                                    <ul class="nav child_menu">
                                        <li><a href="#level1_1">Level One</a>
                                        <li>
                                            <a>Level One<span class="fa fa-chevron-down"></span></a>
                                            <ul class="nav child_menu">
                                                <li class="sub_menu">
                                                    <a href="level2.html">Level Two</a>
                                                </li>
                                                <li>
                                                    <a href="#level2_1">Level Two</a>
                                                </li>
                                                <li>
                                                    <a href="#level2_2">Level Two</a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="#level1_2">Level One</a>
                                        </li>
                                    </ul>
                                </li>*@
                                @*<li><a href="javascript:void(0)"><i class="fa fa-laptop"></i> Landing Page <span class="label label-success pull-right">Coming Soon</span></a></li>*@
                            </ul>
                        </div>

                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <a data-toggle="tooltip" data-placement="top" title="Settings">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Logout" href="login.html">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                        </a>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>

                        <ul class="nav navbar-nav navbar-right">
                            <li class="">
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <img src="" alt="">@User.Identity.GetUserName()
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">

                                    @using (Html.BeginForm("LogOff", "Account", new { area = "Admin" }, FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                                    {
                                        @Html.AntiForgeryToken()
                                    }
                                    <li> @Html.ActionLink("Setting", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })</li>
                                    <li><a href="javascript:document.getElementById('logoutForm').submit()"><i class="fa fa-sign-out pull-right"></i> Log Out</a></li>
                                </ul>
                            </li>                            
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                <!-- top tiles -->
                <div class="row tile_count">

                </div>
                <!-- /top tiles -->

                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        @RenderBody()
                    </div>

                </div>
                <br />




            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    Gentelella - Bootstrap Admin Template by <a href="https://colorlib.com">Colorlib</a>
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    <!-- jQuery -->
    <script src="/Areas/Admin/template/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="/Areas/Admin/template/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="/Areas/Admin/template/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="/Areas/Admin/template/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="/Areas/Admin/template/vendors/Chart.js/dist/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="/Areas/Admin/template/vendors/gauge.js/dist/gauge.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="/Areas/Admin/template/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="/Areas/Admin/template/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="/Areas/Admin/template/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="/Areas/Admin/template/vendors/Flot/jquery.flot.js"></script>
    <script src="/Areas/Admin/template/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="/Areas/Admin/template/vendors/Flot/jquery.flot.time.js"></script>
    <script src="/Areas/Admin/template/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="/Areas/Admin/template/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="/Areas/Admin/template/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="/Areas/Admin/template/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="/Areas/Admin/template/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="/Areas/Admin/template/vendors/DateJS/build/date.js"></script>
    <!-- JQVMap -->
    <script src="/Areas/Admin/template/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="/Areas/Admin/template/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="/Areas/Admin/template/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="/Areas/Admin/template/vendors/moment/min/moment.min.js"></script>
    <script src="/Areas/Admin/template/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>

    <!-- Datatables -->
    <script src="/Areas/Admin/template/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-keytable/js/dataTables.keyTable.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
    <script src="/Areas/Admin/template/vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
    <script src="/Areas/Admin/template/vendors/jszip/dist/jszip.min.js"></script>
    <script src="/Areas/Admin/template/vendors/pdfmake/build/pdfmake.min.js"></script>
    <script src="/Areas/Admin/template/vendors/pdfmake/build/vfs_fonts.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="/Areas/Admin/template/build/js/custom.min.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
